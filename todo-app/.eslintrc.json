{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "no-restricted-globals": ["error", "event", "fdescribe"]
    }
}
